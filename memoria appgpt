# Cargar el HTML final que a√∫n tiene errores
with open("/mnt/data/gastos_HTML_FINAL.html", "r", encoding="utf-8") as f:
    soup = BeautifulSoup(f, "html.parser")

# Eliminar cualquier tabla completa con registros del 27 de marzo
for tabla in soup.find_all("table"):
    if "2025-03-27" in tabla.get_text():
        h2 = tabla.find_previous("h2")
        if h2:
            h2.decompose()
        tabla.decompose()

