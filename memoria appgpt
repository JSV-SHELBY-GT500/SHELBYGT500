from fpdf import FPDF

# Crear clase PDF personalizada
class PDF(FPDF):
    def header(self):
        self.set_font("Arial", "B", 12)
        self.cell(0, 10, "JSV-AUTOPARTES ‚Äì REPORTE DE COTIZACIONES", ln=True, align="C")

    def chapter_title(self, title):
        self.set_font("Arial", "B", 11)
        self.ln(5)
        self.cell(0, 10, title, ln=True)

    def chapter_body(self, body):
        self.set_font("Arial", "", 10)
        self.multi_cell(0, 8, body)

# Crear el PDF
pdf = PDF()
pdf.add_page()

# T√≠tulos y resumen general
pdf.chapter_title("Resumen General")
pdf.chapter_body("Modelo: Honda Civic 1.7L A/T 2002\n"
                 "Total de piezas cotizadas y listas para publicaci√≥n: 41\n"
                 "Valor total estimado de venta: $70,300 MXN")

pdf.chapter_title("Estado General de Publicaci√≥n")
pdf.chapter_body("- Formato Mercado Libre: COMPLETO para cada pieza\n"
                 "- N√∫mero de parte: Confirmado en la mayor√≠a (faltan pocos)\n"
                 "- Garant√≠a: Asignada por tipo de pieza\n"
                 "- Condici√≥n: Usado, sin detalles de funcionamiento\n"
                 "- Im√°genes con fondo limpio: en proceso\n"
                 "- Archivo de publicaci√≥n masiva: en construcci√≥n (.xlsx oficial)\n"
                 "- Control de gastos y egresos: vinculado al sistema visual tipo tac√≥metro Mustang")

# Tabla de piezas
pdf.chapter_title("Piezas Cotizadas y Precios")
pdf.set_font("Arial", "B", 10)
pdf.cell(10, 8, "No.", 1)
pdf.cell(90, 8, "Pieza", 1)
pdf.cell(30, 8, "Precio", 1)
pdf.ln()

pdf.set_font("Arial", "", 10)
for i, (pieza, precio) in enumerate(zip(piezas, precios), 1):
    pdf.cell(10, 8, str(i), 1)
    pdf.cell(90, 8, pieza, 1)
    pdf.cell(30, 8, f"${precio:,}", 1)
    pdf.ln()

# Guardar PDF
