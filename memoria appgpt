# Crear PDF
pdf = FPDF()
pdf.set_auto_page_break(auto=True, margin=15)
pdf.add_page()
pdf.set_font("Arial", size=11)

def limpiar_texto_completo(texto):
    reemplazos = {
        "‚Äì": "-", "‚Äú": '"', "‚Äù": '"', "‚Äô": "'", "‚Äò": "'", "‚Ä¢": "-", "‚Ä¶": "...", "‚Äî": "-", "¬¥": "'", "‚Ä≥": '"',
    }
    for original, reemplazo in reemplazos.items():
        texto = texto.replace(original, reemplazo)
    return texto

# A√±adir contenido del .docx original
for para in doc.paragraphs:
    text = limpiar_texto_completo(para.text.strip())
    if text:
        if para.style.name.startswith('Heading'):
            pdf.set_font("Arial", 'B', size=12)
        else:
            pdf.set_font("Arial", size=11)
        pdf.multi_cell(0, 8, txt=text)
        pdf.ln(1)

# A√±adir piezas nuevas
for titulo, cuerpo in formatos_adicionales.items():
    pdf.set_font("Arial", 'B', size=12)
    pdf.multi_cell(0, 8, titulo)
    pdf.ln(1)
    pdf.set_font("Arial", size=11)
    for linea in cuerpo.split("\n"):
        linea = limpiar_texto_completo(linea.strip())
        if linea:
            pdf.multi_cell(0, 8, txt=linea)
            pdf.ln(0.5)
    pdf.ln(2)

# Guardar PDF
pdf_path = "/mnt/data/JSV_FORMATOS_6_AUTOPARTES.pdf"
pdf.output(pdf_path)

