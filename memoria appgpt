from bs4 import BeautifulSoup

# Cargar el archivo HTML original
with open(html_path, "r", encoding="utf-8") as f:
    soup = BeautifulSoup(f, "html.parser")

# Eliminar la lista general antigua
listas_generales = soup.find_all("h2", string=re.compile("Lista completa de gastos por d√≠a", re.IGNORECASE))
for h2 in listas_generales:
    tabla = h2.find_next("table")
    if tabla:
        tabla.decompose()
    h2.decompose()

# Crear nueva tabla HTML con la lista general corregida
new_h2 = soup.new_tag("h2", style="color:#ffa500;")
new_h2.string = "Lista completa de gastos por d√≠a (Actualizada)"
new_table = soup.new_tag("table", border="1", style="width:100%; border-collapse: collapse;")
header_row = soup.new_tag("tr")
for col in df_gastos.columns:
    th = soup.new_tag("th")
    th.string = col
    header_row.append(th)
new_table.append(header_row)

for _, row in df_gastos.iterrows():
    tr = soup.new_tag("tr")
    for item in row:
        td = soup.new_tag("td")
        td.string = f"${item:,.2f}" if isinstance(item, float) else str(item)
        tr.append(td)
    new_table.append(tr)

# Crear contenedor oculto para la tabla completa
div_lista = soup.new_tag("div", id="lista_gastos", style="display:none;")
div_lista.append(new_h2)
div_lista.append(new_table)

# Crear bot√≥n para mostrar/ocultar
btn_toggle = soup.new_tag("button", onclick="document.getElementById('lista_gastos').style.display='block';")
btn_toggle.string = "Ver lista completa de gastos"

# Crear dise√±o tipo tac√≥metro con animaci√≥n
script = soup.new_tag("script")
script.string = """
function activarTacometro() {
  const aguja = document.getElementById("aguja");
  const total = parseFloat(document.getElementById("total_gastos").textContent);
  const max = 300000;
  const angulo = Math.min(180 * (total / max), 180);
  aguja.style.transform = "rotate(" + angulo + "deg)";
}
"""

estilos = soup.new_tag("style")
estilos.string = """
#tacometro {
  width: 300px;
  height: 150px;
  border-top: 10px solid #333;
  border-left: 10px solid transparent;
  border-right: 10px solid transparent;
  border-radius: 150px 150px 0 0;
  position: relative;
  margin: 20px auto;
}
#aguja {
  width: 4px;
  height: 140px;
  background: red;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform-origin: bottom center;
  transform: rotate(0deg);
  transition: transform 2s ease-in-out;
}
"""

tacometro_div = soup.new_tag("div", id="tacometro")
aguja_div = soup.new_tag("div", id="aguja")
tacometro_div.append(aguja_div)

total_gastos = df_gastos["Monto"].sum()
span_total = soup.new_tag("span", id="total_gastos", style="display:none;")
span_total.string = str(total_gastos)

btn_tacometro = soup.new_tag("button", onclick="activarTacometro()")
btn_tacometro.string = "Encender Tac√≥metro de Gastos"

# Insertar en el body
soup.body.append(estilos)
soup.body.append(script)
soup.body.append(tacometro_div)
soup.body.append(span_total)
soup.body.append(btn_tacometro)
soup.body.append(btn_toggle)
soup.body.append(div_lista)

# Guardar HTML actualizado
output_html_path = "/mnt/data/gastos_actualizado_con_tacometro.html"
with open(output_html_path, "w", encoding="utf-8") as f:
    f.write(str(soup))

