# Regenerar el PDF asegurando que se eliminen todos los caracteres problem√°ticos
from fpdf import FPDF

# Funci√≥n para limpiar caracteres especiales antes de generar el PDF
def clean_text(text):
    # Reemplazar caracteres problem√°ticos con equivalentes ASCII
    replacements = {
        "‚Äì": "-", "‚Äò": "'", "‚Äô": "'", "‚Äú": '"', "‚Äù": '"', "‚Ä¢": "-", "‚Äî": "-", "¬¥": "'", "‚Ä≥": '"'
    }
    for original, new in replacements.items():
        text = text.replace(original, new)
    return text

# Crear el PDF limpio sin caracteres problem√°ticos
pdf = FPDF()
pdf.set_auto_page_break(auto=True, margin=15)
pdf.add_page()
pdf.set_font("Arial", size=11)

# T√≠tulo
pdf.set_font("Arial", 'B', 14)
pdf.cell(0, 10, "FORMATO DE COTIZACI√ìN JSV-AUTOPARTES", ln=True, align="C")
pdf.ln(5)

# A√±adir las piezas y formatos
