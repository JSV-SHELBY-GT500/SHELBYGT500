Mostrar siempre los detalles
from bs4 import BeautifulSoup
import pandas as pd
import re

# Cargar y procesar el archivo HTML para extraer los datos por d√≠a (parte superior v√°lida)
html_path = "/mnt/data/gastos sirven.html"
with open(html_path, "r", encoding="utf-8") as file:
    soup = BeautifulSoup(file, "html.parser")

# Extraer todos los bloques por d√≠a que contienen tablas
dias = soup.find_all("h2", style=re.compile("color:#ffa500;"))

gastos_por_dia = []
for dia in dias:
    titulo = dia.text.strip()
    tabla = dia.find_next("table")
    if not tabla:
        continue
    filas = tabla.find_all("tr")[1:]  # saltar encabezado
    for fila in filas:
        celdas = fila.find_all("td")
        if len(celdas) == 3:
            fecha = celdas[0].text.strip()
            descripcion = celdas[1].text.strip()
            monto = float(celdas[2].text.strip().replace("$", "").replace(",", ""))
            gastos_por_dia.append({"Fecha": fecha, "Descripci√≥n": descripcion, "Monto": monto})

# Crear DataFrame limpio
df_gastos = pd.DataFrame(gastos_por_dia)

# Generar resumen total por d√≠a desde el DataFrame
resumen = df_gastos.groupby("Fecha").agg({"Monto": "sum"}).reset_index()

import ace_tools as tools; tools.display_dataframe_to_user(name="Lista general corregida de gastos", dataframe=df_gastos)
