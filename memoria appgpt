# Corregimos el error re-limpiando caracteres problem√°ticos m√°s a fondo
def limpiar_todo(texto):
    return limpiar_texto_completo(texto).encode("ascii", "ignore").decode()

# Regenerar PDF sin caracteres conflictivos
pdf = FPDF()
pdf.set_auto_page_break(auto=True, margin=15)
pdf.add_page()
pdf.set_font("Arial", size=11)

# A√±adir contenido del .docx limpio
for para in doc.paragraphs:
    text = limpiar_todo(para.text.strip())
    if text:
        if para.style.name.startswith('Heading'):
            pdf.set_font("Arial", 'B', size=12)
        else:
            pdf.set_font("Arial", size=11)
        pdf.multi_cell(0, 8, txt=text)
        pdf.ln(1)

# A√±adir nuevas piezas ya limpias
for titulo, cuerpo in formatos_adicionales.items():
    pdf.set_font("Arial", 'B', size=12)
    pdf.multi_cell(0, 8, limpiar_todo(titulo))
    pdf.ln(1)
    pdf.set_font("Arial", size=11)
    for linea in cuerpo.split("\n"):
        linea = limpiar_todo(linea.strip())
        if linea:
            pdf.multi_cell(0, 8, txt=linea)
            pdf.ln(0.5)
    pdf.ln(2)

# Guardar PDF final
pdf_path = "/mnt/data/JSV_FORMATOS_6_AUTOPARTES.pdf"
pdf.output(pdf_path)

