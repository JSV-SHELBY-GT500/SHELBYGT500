from fpdf import FPDF

# Lista final de las 21 piezas con su nombre y precio confirmado
piezas_confirmadas = [
    ("Radiador", 1350),
    ("Booster de freno", 1450),
    ("Bomba con dep√≥sito de l√≠quido de freno", 750),
    ("Faro delantero derecho", 1100),
    ("Faro delantero izquierdo", 1100),
    ("Calavera trasera derecha", 1100),
    ("Calavera trasera izquierda", 1100),
    ("Fascia delantera", 1800),
    ("Fascia trasera", 1800),
    ("Cristal puerta copiloto coup√©", 1500),
    ("Parabrisas", 2100),
    ("Medall√≥n", 2100),
    ("Retrovisor izquierdo", 800),
    ("Retrovisor derecho", 800),
    ("Retrovisor central", 650),
    ("Volante sin airbag", 950),
    ("Airbag", 1100),
    ("Visera derecha", 500),
    ("Visera izquierda", 500),
    ("Cajuela", 2000),
    ("Cofre sin cortar con toma aire", 2500)
]

# Crear PDF
pdf = FPDF()
pdf.set_auto_page_break(auto=True, margin=15)
pdf.add_page()
pdf.set_font("Arial", size=11)

pdf.set_font("Arial", 'B', 14)
pdf.cell(0, 10, "FORMATO DE COTIZACI√ìN JSV-AUTOPARTES", ln=True, align="C")
pdf.ln(5)

for pieza, precio in piezas_confirmadas:
    pdf.set_font("Arial", 'B', 12)
    pdf.cell(0, 10, f"{pieza.upper()} ‚Äì ${precio}", ln=True)
    pdf.set_font("Arial", size=11)
    pdf.multi_cell(0, 8, f"""JSV-AUTOPARTES

MODELO DE DESMONTADO:
Honda Civic 1.7L A/T 2002

MODELO A INSTALAR:
NO ESPECIFICADO AL COTIZAR

