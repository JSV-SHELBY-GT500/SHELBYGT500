        "Usado",  # Condici√≥n
        "",  # Imagen URL (sin imagen)
        "Autopartes > Motor > Alternadores",  # Categor√≠a (ajustar si es necesario)
        "",  # SKU (si no se usa)
        "Env√≠o gratis",  # Condiciones de Env√≠o
        "Honda",  # Marca
        "30 d√≠as",  # Garant√≠a
        "",  # Variaciones (si aplica)
        "",  # C√≥digos Internacionales
        ""  # ID de Variaci√≥n (si no aplica)
    ])

