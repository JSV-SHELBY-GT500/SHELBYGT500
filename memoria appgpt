from bs4 import BeautifulSoup
import pandas as pd
import re

# Cargar HTML base
html_path = "/mnt/data/gastos_REPARADO_FINAL_V2.html"
with open(html_path, "r", encoding="utf-8") as f:
    soup = BeautifulSoup(f, "html.parser")

# ELIMINAR TODO LO DEL 27 DE MARZO
for tag in soup.find_all(string=re.compile("2025-03-27|jueves 27 de marzo", re.IGNORECASE)):
    parent = tag.find_parent(["h2", "tr"])
    if parent:
        parent.decompose()

# ELIMINAR TAC√ìMETROS PREVIOS Y BOTONES
for elem_id in ["tacometro", "aguja", "gauge-gastos", "total_gastos", "btn_tacometro"]:
    elem = soup.find(id=elem_id)
    if elem:
        elem.decompose()

for tag in soup.find_all(["script", "style"]):
    if "gauge" in tag.text or "tacometro" in tag.text or "aguja" in tag.text:
        tag.decompose()

# ELIMINAR TABLAS GENERALES DUPLICADAS
tablas = soup.find_all("table")
encabezados = []
for tabla in tablas:
    h2 = tabla.find_previous("h2")
    if h2 and "Lista completa de gastos" in h2.text:
        encabezados.append((h2, tabla))
for h2, tabla in encabezados[1:]:
    h2.decompose()
    tabla.decompose()

# OCULTAR TABLA GENERAL
lista_general = soup.find("div", {"id": "lista_gastos"})
if lista_general:
    lista_general["style"] = "display:none;"

# CREAR TAC√ìMETRO FINAL CANVAS
canvas_html = BeautifulSoup(f"""
<script src="https://cdn.jsdelivr.net/gh/Mikhus/canvas-gauges/gauge.min.js"></script>
<canvas data-type="radial-gauge"
        data-width="300"
        data-height="300"
        data-units="$MXN"
        data-min-value="0"
        data-max-value="300000"
        data-major-ticks="0,50000,100000,150000,200000,250000,300000"
        data-minor-ticks="5"
        data-stroke-ticks="true"
        data-highlights='[{{"from": 200000, "to": 300000, "color": "rgba(200, 50, 50, .75)"}}]'
        data-color-plate="#222"
        data-color-major-ticks="#f5f5f5"
        data-color-minor-ticks="#ddd"
        data-color-title="#fff"
        data-color-units="#ccc"
        data-color-numbers="#eee"
        data-color-needle-start="red"
        data-color-needle-end="red"
        data-value="0"
        id="gauge-gastos">
</canvas>
""", "html.parser")

# CALCULAR TOTAL GASTOS
gastos = []
for tabla in soup.find_all("table"):
    headers = [th.get_text().strip().lower() for th in tabla.find_all("th")]
    if "fecha" in headers and "descripci√≥n" in headers and "monto" in headers:
        for fila in tabla.find_all("tr")[1:]:
            cols = fila.find_all("td")
            if len(cols) == 3:
                try:
                    monto = float(cols[2].text.strip().replace("$", "").replace(",", ""))
                    gastos.append(monto)
                except:
                    pass
total_gasto = round(sum(gastos), 2)

# A√ëADIR VALOR AL TAC√ìMETRO
canvas_html.canvas["data-value"] = str(total_gasto)

# BOT√ìN DE ENCENDIDO ROJO
btn_html = BeautifulSoup("""
<style>
#btn_encender {
  width: 60px;
  height: 60px;
  background: red;
  border-radius: 50%;
  border: none;
  box-shadow: 0 0 10px #000;
  display: block;
  margin: 10px auto;
}
</style>
<button id="btn_encender" onclick="document.getElementById('lista_gastos').style.display = 'block';">
</button>
""", "html.parser")

# INSERTAR TODO ANTES DE LA TABLA
if lista_general:
    lista_general.insert_before(canvas_html)
    lista_general.insert_before(btn_html)

# GUARDAR HTML FINAL
html_final_path = "/mnt/data/gastos_HTML_FINAL.html"
with open(html_final_path, "w", encoding="utf-8") as f:
    f.write(str(soup))

html_final_path
