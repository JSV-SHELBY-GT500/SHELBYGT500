from bs4 import BeautifulSoup

# Cargar el HTML existente con errores
html_path_fix = "/mnt/data/gastos_actualizado_con_tacometro.html"
with open(html_path_fix, "r", encoding="utf-8") as f:
    soup = BeautifulSoup(f, "html.parser")

# Eliminar cualquier fila o t√≠tulo relacionado con el d√≠a 27
for tag in soup.find_all(string=lambda t: "27" in t):
    parent = tag.find_parent(["h2", "tr"])
    if parent:
        parent.decompose()

# Quitar el tac√≥metro anterior completamente
for elem_id in ["tacometro", "aguja", "total_gastos", "activarTacometro"]:
    found = soup.find(id=elem_id)
    if found:
        found.decompose()
        
# Quitar scripts y estilos anteriores relacionados al tac√≥metro
for tag in soup.find_all(["script", "style"]):
    if "tacometro" in tag.text or "aguja" in tag.text:
        tag.decompose()

# Quitar el bot√≥n viejo
for btn in soup.find_all("button"):
    if "Encender Tac√≥metro" in btn.text:
        btn.decompose()

# Crear nuevo estilo y tac√≥metro corregido
new_style = soup.new_tag("style")
new_style.string = """
#tacometro {
  width: 300px;
  height: 150px;
  background: radial-gradient(circle at bottom, #111 60%, #000 100%);
  border-top-left-radius: 150px;
  border-top-right-radius: 150px;
  margin: 30px auto;
  position: relative;
}
#aguja {
  width: 6px;
  height: 140px;
  background: red;
  position: absolute;
  bottom: 0;
  left: 50%;
