from bs4 import BeautifulSoup
import shutil

# Ruta base del HTML a modificar
html_base_path = "/mnt/data/gastos_REPARADO_FINAL_V2.html"

# Crear una copia de respaldo antes de modificar
shutil.copy(html_base_path, "/mnt/data/gastos_REPARADO_BACKUP.html")

# Cargar el HTML
with open(html_base_path, "r", encoding="utf-8") as f:
    soup = BeautifulSoup(f, "html.parser")

# Eliminar todo lo relacionado con el 27 de marzo (encabezados o filas)
for tag in soup.find_all(string=lambda t: "2025-03-27" in t or "jueves 27 de marzo" in t.lower()):
    parent = tag.find_parent(["h2", "tr"])
    if parent:
        parent.decompose()

# Eliminar duplicados de la lista general si existiera m√°s de una tabla similar
vistas_generales = []
for tabla in soup.find_all("table"):
    encabezado = tabla.find_previous("h2")
    if encabezado and "Lista completa de gastos" in encabezado.text:
        vistas_generales.append((encabezado, tabla))

# Conservar solo una
for encabezado, tabla in vistas_generales[1:]:
    encabezado.decompose()
    tabla.decompose()

# Reemplazar el tac√≥metro con el nuevo de canvas (proyecto speedometer)
speedometer_script = """
<script src="https://cdn.jsdelivr.net/gh/Mikhus/canvas-gauges/gauge.min.js"></script>
<canvas data-type="radial-gauge"
        data-width="300"
        data-height="300"
        data-units="$MXN"
        data-min-value="0"
        data-max-value="300000"
        data-major-ticks="0,50000,100000,150000,200000,250000,300000"
        data-minor-ticks="5"
        data-stroke-ticks="true"
        data-highlights='[{"from": 200000, "to": 300000, "color": "rgba(200, 50, 50, .75)"}]'
        data-color-plate="#222"
        data-color-major-ticks="#f5f5f5"
        data-color-minor-ticks="#ddd"
        data-color-title="#fff"
        data-color-units="#ccc"
        data-color-numbers="#eee"
        data-color-needle-start="red"
        data-color-needle-end="red"
        data-value="{gasto_total}"
        id="gauge-gastos">
</canvas>
"""

# Calcular total actualizado de gastos
import pandas as pd

gastos_rows = []
for tabla in soup.find_all("table"):
    headers = [th.get_text().strip().lower() for th in tabla.find_all("th")]
    if "fecha" in headers and "descripci√≥n" in headers and "monto" in headers:
        for fila in tabla.find_all("tr")[1:]:
            celdas = fila.find_all("td")
            if len(celdas) == 3:
                try:
                    monto = float(celdas[2].text.strip().replace("$", "").replace(",", ""))
                    gastos_rows.append(monto)
                except:
                    continue

gasto_total = round(sum(gastos_rows), 2)

# Insertar tac√≥metro real antes del bot√≥n
canvas_html = BeautifulSoup(speedometer_script.replace("{gasto_total}", str(gasto_total)), "html.parser")
btn = soup.find("button", id="btn_tacometro")
if btn:
    btn.insert_before(canvas_html)

# Guardar archivo final
html_final_path = "/mnt/data/gastos_TACOMETRO_REAL_OK.html"
with open(html_final_path, "w", encoding="utf-8") as f:
    f.write(str(soup))

html_final_path
