    # Formato Mercado Libre
    pdf.set_font("Arial", 'B', 12)
    pdf.cell(0, 10, "========== FORMATO MERCADO LIBRE ‚Äì PLANTILLA OFICIAL ==========", ln=True)
    pdf.set_font("Arial", size=11)
