from bs4 import BeautifulSoup

# Recargar archivo base con errores
html_base = "/mnt/data/gastos_actualizado_con_tacometro.html"
with open(html_base, "r", encoding="utf-8") as f:
    soup = BeautifulSoup(f, "html.parser")

# Eliminar TODAS las filas y encabezados que mencionen el d√≠a 27
for tag in soup.find_all(string=lambda t: "27" in t):
    parent = tag.find_parent(["h2", "tr"])
    if parent:
        parent.decompose()

# Eliminar tablas repetidas de lista completa
tablas = soup.find_all("table")
vistas_repetidas = [tabla for tabla in tablas if tabla.find_previous("h2") and "Lista completa" in tabla.find_previous("h2").text]
for tabla in vistas_repetidas:
    tabla.find_previous("h2").decompose()
    tabla.decompose()

# Borrar tac√≥metro anterior y scripts/estilos relacionados
for elem_id in ["tacometro", "aguja", "total_gastos"]:
    found = soup.find(id=elem_id)
    if found:
        found.decompose()

for tag in soup.find_all(["script", "style"]):
    if "tacometro" in tag.text or "aguja" in tag.text:
        tag.decompose()

for btn in soup.find_all("button"):
    if "Encender Tac√≥metro" in btn.text or btn.get("id") == "btn_tacometro":
        btn.decompose()

# Crear tac√≥metro visual nuevo con n√∫meros
new_style = soup.new_tag("style")
new_style.string = """
#tacometro {
  width: 300px;
  height: 150px;
  position: relative;
  margin: 40px auto;
  background: #222;
  border-top-left-radius: 150px;
  border-top-right-radius: 150px;
  border: 6px solid #444;
}
#aguja {
  width: 4px;
  height: 130px;
  background: red;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform-origin: bottom center;
  transform: rotate(0deg);
  transition: transform 2s ease-out;
}
#marcas {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  color: white;
  font-size: 12px;
}
#marcas div {
  position: absolute;
  transform-origin: bottom center;
  bottom: 10px;
  left: 50%;
}
#btn_tacometro {
  display: block;
  margin: 10px auto;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background: red;
  border: none;
  cursor: pointer;
  box-shadow: 0 0 5px #000;
}
"""

# Nuevo script para animar tac√≥metro y mostrar tabla
new_script = soup.new_tag("script")
new_script.string = """
function encenderTacometro() {
  const aguja = document.getElementById("aguja");
  const total = parseFloat(document.getElementById("total_gastos").textContent);
  const max = 300000;
  const angulo = Math.min(180 * (total / max), 180);
  aguja.style.transform = "rotate(" + angulo + "deg)";
  document.getElementById("lista_gastos").style.display = 'block';
}
"""

# Crear marcas visuales del tac√≥metro
marcas = soup.new_tag("div", id="marcas")
for i in range(0, 301000, 50000):
    div = soup.new_tag("div")
    ang = (i / 300000) * 180
    div["style"] = f"transform: rotate({ang}deg) translateY(-130px);"
    div.string = f"${i//1000}k"
    marcas.append(div)

# Crear tac√≥metro visual
tacometro = soup.new_tag("div", id="tacometro")
aguja = soup.new_tag("div", id="aguja")
tacometro.append(aguja)
tacometro.append(marcas)

# Total de gastos reales
from pandas import DataFrame
from datetime import datetime
import re

# Volver a cargar datos buenos del archivo anterior
gastos_data = []
tables = soup.find_all("table")
for table in tables:
    headers = [th.get_text().strip() for th in table.find_all("th")]
    if headers == ["Fecha", "Descripci√≥n", "Monto"]:
        rows = table.find_all("tr")[1:]
        for row in rows:
            cols = row.find_all("td")
            if len(cols) == 3:
                fecha = cols[0].text.strip()
                descripcion = cols[1].text.strip()
                monto_txt = cols[2].text.strip().replace("$", "").replace(",", "")
                try:
                    monto = float(monto_txt)
                    gastos_data.append({"Fecha": fecha, "Descripci√≥n": descripcion, "Monto": monto})
                except:
                    continue

df_gastos_corregido = DataFrame(gastos_data)
total_gasto = df_gastos_corregido["Monto"].sum()

# Insertar total oculto
total_span = soup.new_tag("span", id="total_gastos", style="display:none;")
total_span.string = str(total_gasto)

# Bot√≥n encendido redondo rojo
btn_red = soup.new_tag("button", id="btn_tacometro", onclick="encenderTacometro()")
btn_red.string = ""

# Asegurar que la tabla completa est√© oculta por defecto
div_lista = soup.find("div", {"id": "lista_gastos"})
if div_lista:
    div_lista["style"] = "display:none;"

# Insertar elementos limpios
soup.body.insert(0, new_style)
soup.body.insert(1, new_script)
soup.body.insert(2, tacometro)
soup.body.insert(3, total_span)
soup.body.insert(4, btn_red)

# Guardar HTML limpio y funcional
html_output = "/mnt/data/gastos_REPARADO_FINAL.html"
with open(html_output, "w", encoding="utf-8") as f:
    f.write(str(soup))

