# Soluci√≥n: limpiar caracteres conflictivos antes de generar PDF

def limpiar_texto(texto):
    reemplazos = {
        "‚Äì": "-", "‚Äú": '"', "‚Äù": '"', "‚Äô": "'", "‚Äò": "'", "‚Ä¢": "-", "‚Ä¶": "...", "‚Äî": "-", "¬¥": "'", "‚Ä≥": '"',
    }
    for original, reemplazo in reemplazos.items():
        texto = texto.replace(original, reemplazo)
    return texto.encode("ascii", "ignore").decode()

# Regenerar PDF con texto limpio
pdf = FPDF()
pdf.set_auto_page_break(auto=True, margin=15)
pdf.add_page()
pdf.set_font("Arial", size=11)

pdf.set_font("Arial", 'B', 14)
pdf.cell(0, 10, "FORMATO DE COTIZACI√ìN JSV-AUTOPARTES", ln=True, align="C")
pdf.ln(5)

for pieza, precio in piezas_confirmadas:
    pdf.set_font("Arial", 'B', 12)
    titulo = limpiar_texto(f"{pieza.upper()} ‚Äì ${precio}")
    pdf.cell(0, 10, titulo, ln=True)
    pdf.set_font("Arial", size=11)
    contenido = f"""JSV-AUTOPARTES

MODELO DE DESMONTADO:
Honda Civic 1.7L A/T 2002

