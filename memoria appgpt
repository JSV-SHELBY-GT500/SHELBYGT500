from pptx import Presentation
from pptx.util import Inches, Pt
from pptx.dml.color import RGBColor

# Crear presentaci√≥n
prs = Presentation()
prs.slide_width = Inches(16)
prs.slide_height = Inches(9)

def add_slide(title, content_list):
    slide = prs.slides.add_slide(prs.slide_layouts[5])
    title_box = slide.shapes.title
    title_box.text = title
    title_box.text_frame.paragraphs[0].font.size = Pt(40)
    title_box.text_frame.paragraphs[0].font.bold = True
    title_box.text_frame.paragraphs[0].font.color.rgb = RGBColor(255, 165, 0)

    left = Inches(1)
    top = Inches(1.5)
    width = Inches(14)
    height = Inches(6)
    content_box = slide.shapes.add_textbox(left, top, width, height)
    tf = content_box.text_frame
    tf.word_wrap = True
    for item in content_list:
        p = tf.add_paragraph()
        p.text = f"- {item}"
        p.font.size = Pt(20)
        p.font.color.rgb = RGBColor(255, 255, 255)

# 1. Resumen de cotizaciones autom√°ticas (inbox y WhatsApp)
add_slide("Resumen Cotizaciones Autom√°ticas", [
    "Cotizaciones generadas desde captura de pantalla:",
    "‚Ä¢ WhatsApp: 6",
    "‚Ä¢ Inbox (Messenger): 5",
    "Total: 11 cotizaciones autom√°ticas",
    "Piezas con flujo completo desde imagen: Alternador, Caja fusibles, Bobina, M√≥dulo crucero, Filtro, Bomba hidr√°ulica"
])

# 2. Registro de piezas y precios
add_slide("Piezas Registradas y Monto Total", [
    "Piezas registradas ayer: 41",
    "Modo de registro: Identificaci√≥n visual + nombre + precio confirmado",
    "Piezas a subir hoy: 41",
    "Precio total para subir a Mercado Libre: $70,300 MXN",
    "Sumando precio de Motor $11,000 y Transmisi√≥n $9,000 => TOTAL FINAL: $90,300 MXN"
])

# 3. SHELBY GT 500 PC y JSVAPP
add_slide("Software en Funcionamiento", [
    "JSVAPP: Registro, edici√≥n y publicaci√≥n de autopartes activado",
    "SHELBY GT 500 PC: Control maestro de publicaciones, an√°lisis y automatizaci√≥n",
    "Integraci√≥n con Google Drive, Chat, Formatos y ML",
    "Sistema de predicci√≥n, seguimiento de piezas y asistente visual activo"
])

