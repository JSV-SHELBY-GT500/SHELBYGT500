# Regenerar el PDF limpiando los caracteres problem√°ticos
from fpdf import FPDF

# Funci√≥n para limpiar texto de caracteres especiales
def clean_text(text):
    # Reemplazar caracteres problem√°ticos
    replacements = {
        "‚Äì": "-", "‚Äò": "'", "‚Äô": "'", "‚Äú": '"', "‚Äù": '"', "‚Ä¢": "-", "‚Äî": "-", "¬¥": "'", "‚Ä≥": '"', "‚Ä¢": "-"
    }
    for original, new in replacements.items():
        text = text.replace(original, new)
    return text

